{
  "name": "book_my_doctor_app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "prepare": "husky",
    "prebuild": "expo prebuild --clean",
    "eas:update": "eas update --clear-cache",
    "build": "eas build -p all --clear-cache",

    "build:android:bundle": "eas build -p android --profile bundle --clear-cache",
    "build:android:bundle:local": "eas build -p android --profile bundle --local",
    "build:android": "eas build -p android --profile preview --clear-cache",
    "build:android:local": "eas build -p android --profile preview --local",
    "build:run:android": "eas build:run -p android --profile preview",
    "android": "expo run:android --no-build-cache",
    "android:dev": "expo run:android",

    "build:ios:dev": "eas build -p ios --profile development --local --clear-cache",
    "build:ios:bundle": "eas build -p ios --profile bundle --clear-cache",
    "build:ios:bundle:local": "eas build -p ios --profile bundle --local",
    "build:ios": "eas build -p ios --profile preview --clear-cache",
    "build:ios:local": "eas build -p ios --profile preview --local",
    "build:run:ios": "eas build:run -p ios --profile preview",
    "ios": "expo run:ios --no-build-cache",
    "ios:dev": "expo run:ios",

    "start": "expo start",
    "start:dev": "expo start -c",
    "web": "expo start --web",
    "android:release": "expo run:android --variant release",
    "lint:prettier-fix": "prettier --write '{screens,components,utils,navigation}/**/*.{js,jsx}'",
    "lint:prettier": "prettier '{screens,components,utils,navigation,store}/**/*.{js,jsx}' --config .prettierrc --check",
    "lint:eslint": "eslint '{screens,components,utils,navigation,store}/**/*.{js,jsx}' -c .eslintrc.js",
    "lint": "concurrently 'npm run lint:prettier' 'npm run lint:eslint'",
    "lint:eslint-fix": "npm run lint:eslint -- --fix",
    "lint:fix": "concurrently \"npm run lint:prettier-fix\" \"npm run lint:eslint-fix\"",
    "lint:staged": "lint-staged",
    "rnddebug": "open 'rndebugger://localhost?host=localhost&port=8081'"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "8.0.1",
    "@react-native-firebase/app": "^21.1.1",
    "@react-native-firebase/messaging": "^21.1.1",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.2.7",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "axios": "^1.7.7",
    "expo": "~51.0.34",
    "expo-application": "~5.9.1",
    "expo-auth-session": "~5.5.2",
    "expo-build-properties": "^0.12.5",
    "expo-crypto": "~13.0.2",
    "expo-google-fonts": "^0.0.0",
    "expo-image-picker": "~15.0.7",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "^3.0.7",
    "expo-updates": "~0.25.27",
    "expo-web-browser": "~13.0.3",
    "firebase": "^10.13.2",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "react-native-alert-notification": "^0.4.0",
    "react-native-date-picker": "^5.0.4",
    "react-native-dotenv": "^3.4.11",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-picker-select": "^9.1.3",
    "react-native-rating-element": "^5.4.0",
    "react-native-razorpay": "^2.3.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",
    "react-native-select-dropdown": "^4.0.1",
    "react-native-svg": "15.2.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-vector-icons": "^10.1.0",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/eslint-parser": "^7.24.7",
    "concurrently": "^8.2.2",
    "eslint": "^8.42.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "expo-dev-client": "~4.0.27",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "react-native-devsettings": "^1.0.5"
  },
  "private": true
}
