{
  "version": 2,
  "builds": [
    {
      "src": "./src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "outputDirectory": "./dist",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./dist/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    }
  ]
}