{
  "version": 2,
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "outputDirectory": "./dist",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./dist/main.js",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE",
        "PATCH",
        "OPTIONS"
      ]
    }
  ]
}
